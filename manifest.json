{
  "name": "Southwest Checkin",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Check in on Southwest biotch!",
  "permissions": [
    "http://www.southwest.com/",
    "storage",
    "tabs",
    "alarms"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "browser_action": {
    "default_title": "show",
    "default_icon": {
      "19": "img/icon.png",
      "38": "img/icon.png"
    }
  },
  "icons": {
    "16": "img/icon.png",
    "48": "img/icon.png",
    "128": "img/icon.png"
  },
  "background": {
    "persistent": false,
  	"scripts": [
  		"js/vendor/jquery.js",
  		"js/vendor/underscore.js",
      "js/vendor/moment.js",
  		"js/background.js"
  	]
  }
}
